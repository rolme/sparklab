    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-image-gallery": "^0.8.8",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.1.4",
    "react-scrollspy": "^3.3.5",
    "react-spinners": "^0.3.2",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-action": "^1.2.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.0.1",
